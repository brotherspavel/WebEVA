{
    "title": "Info-11",
    "timeout": 15000,
    "steps": [
        {
            "type": "setViewport",
            "width": 910,
            "height": 754,
            "deviceScaleFactor": 1,
            "isMobile": false,
            "hasTouch": false,
            "isLandscape": false
        },
        {
            "type": "navigate",
            "url": "https://www.google.com/",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://www.google.com/",
                    "title": "Google"
                }
            ]
        },
        {
            "type": "navigate",
            "url": "https://www.babycenter.com/",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://www.babycenter.com/",
                    "title": ""
                }
            ]
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                [
                    "aria/Show main navigation menu"
                ],
                [
                    "button.menuIcon"
                ],
                [
                    "xpath///*[@id=\"appBarContent\"]/div[1]/button[1]"
                ],
                [
                    "pierce/button.menuIcon"
                ]
            ],
            "offsetY": 23,
            "offsetX": 21
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                [
                    "aria/How can we help?"
                ],
                [
                    "div.maxWidth input"
                ],
                [
                    "xpath///*[@id=\"__next\"]/div[3]/section/div/div[1]/div/div/form/div/input"
                ],
                [
                    "pierce/div.maxWidth input"
                ]
            ],
            "offsetY": 25.5,
            "offsetX": 84
        },
        {
            "type": "change",
            "value": "baby sleep schedule",
            "selectors": [
                [
                    "aria/How can we help?"
                ],
                [
                    "div.maxWidth input"
                ],
                [
                    "xpath///*[@id=\"__next\"]/div[3]/section/div/div[1]/div/div/form/div/input"
                ],
                [
                    "pierce/div.maxWidth input"
                ]
            ],
            "target": "main"
        },
        {
            "type": "keyDown",
            "target": "main",
            "key": "Enter"
        },
        {
            "type": "keyUp",
            "key": "Enter",
            "target": "main"
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                [
                    "div:nth-of-type(3) > div:nth-of-type(3) div.resultTitle > b:nth-of-type(2)"
                ],
                [
                    "xpath///*[@id=\"__next\"]/div[5]/div/div[1]/div/div/main/div[3]/div[3]/div/a/div[2]/div/div[1]/b[2]"
                ],
                [
                    "pierce/div:nth-of-type(3) > div:nth-of-type(3) div.resultTitle > b:nth-of-type(2)"
                ]
            ],
            "offsetY": 7,
            "offsetX": 14.8125,
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://www.babycenter.com/baby/sleep/baby-sleep-basics-3-to-6-months_7656",
                    "title": ""
                }
            ]
        }
    ]
}
