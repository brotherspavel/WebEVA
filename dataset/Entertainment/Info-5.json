{
    "title": "Info-5",
    "timeout": 15000,
    "steps": [
        {
            "type": "setViewport",
            "width": 910,
            "height": 754,
            "deviceScaleFactor": 1,
            "isMobile": false,
            "hasTouch": false,
            "isLandscape": false
        },
        {
            "type": "navigate",
            "url": "https://www.google.com/",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://www.google.com/",
                    "title": "Google"
                }
            ]
        },
        {
            "type": "navigate",
            "url": "https://www.accuweather.com/",
