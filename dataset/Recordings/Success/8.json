                }
            ]
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                [
                    "aria/Search Wikipedia"
                ],
                [
                    "#searchInput"
                ],
                [
                    "xpath///*[@id=\"searchInput\"]"
                ],
                [
                    "pierce/#searchInput"
                ]
            ],
            "offsetY": 19.15625,
            "offsetX": 139.09375
        },
        {
            "type": "change",
            "value": "George Washington",
            "selectors": [
                [
                    "aria/Search Wikipedia"
                ],
                [
                    "#searchInput"
                ],
                [
                    "xpath///*[@id=\"searchInput\"]"
                ],
                [
                    "pierce/#searchInput"
                ]
            ],
            "target": "main"
        },
        {
            "type": "keyDown",
            "target": "main",
            "key": "Enter",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://en.wikipedia.org/wiki/George_Washington",
