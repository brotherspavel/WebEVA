{
    "title": "9",
    "timeout": 15000,
    "steps": [
        {
            "type": "setViewport",
            "width": 1383,
            "height": 911,
            "deviceScaleFactor": 1,
            "isMobile": false,
            "hasTouch": false,
            "isLandscape": false
        },
        {
            "type": "navigate",
            "url": "https://www.google.com.hk/",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://www.google.com.hk/",
                    "title": "Google"
                }
            ]
        },
        {
            "type": "navigate",
            "url": "https://www.merriam-webster.com/",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://www.merriam-webster.com/",
                    "title": "Merriam-Webster: America's Most Trusted Dictionary"
                }
            ]
        },
        {
            "type": "click",
            "target": "main",
            "selectors": [
                [
                    "#home-search-term"
                ],
                [
                    "xpath///*[@id=\"home-search-term\"]"
                ],
                [
                    "pierce/#home-search-term"
                ]
            ],
            "offsetY": 25,
            "offsetX": 76
        },
        {
            "type": "change",
            "value": "Quantum Computing",
            "selectors": [
                [
                    "#home-search-term"
                ],
                [
                    "xpath///*[@id=\"home-search-term\"]"
                ],
                [
                    "pierce/#home-search-term"
                ]
            ],
            "target": "main"
        },
        {
            "type": "keyDown",
            "target": "main",
            "key": "Enter",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://www.merriam-webster.com/dictionary/quantum%20computing",
                    "title": "Quantum computing Definition & Meaning - Merriam-Webster"
                }
            ]
        },
        {
            "type": "keyUp",
            "key": "Enter",
            "target": "main"
        }
    ]
}
